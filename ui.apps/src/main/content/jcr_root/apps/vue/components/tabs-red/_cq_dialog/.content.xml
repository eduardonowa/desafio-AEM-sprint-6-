<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties Tabs"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured"
               sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <header-properties jcr:primaryType="nt:unstructured"
                                  jcr:title="Header"
                                  sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <description cq-msm-lockable="./description"
                                        fieldLabel="Description text"
                                        jcr:primaryType="nt:unstructured"
                                        name="./description"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <colorDescription cq-msm-lockable="./colorDescription"
                                             fieldLabel="Color"
                                             jcr:primaryType="nt:unstructured"
                                             name="./colorDescription"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <fontFamilyDescription cq-msm-lockable="./fontFamilyDescription"
                                                  fieldLabel="Description Font Family:"
                                                  jcr:primaryType="nt:unstructured"
                                                  name="./fontFamilyDescription"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                  vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyDescription"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyDescription"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyDescription"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyDescription>
                           <fontSizeDescription cq-msm-lockable="./fontSizeDescription"
                                                fieldLabel="Description Font-Size (px)"
                                                jcr:primaryType="nt:unstructured"
                                                name="./fontSizeDescription"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                           <textAlignHeader cq-msm-lockable="./textAlignHeader"
                                            defaultValue="textAlignHeader"
                                            fieldLabel="Content Position:"
                                            jcr:primaryType="nt:unstructured"
                                            name="./textAlignHeader"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            value="false">
                              <items jcr:primaryType="nt:unstructured">
                                 <flex-start jcr:primaryType="nt:unstructured" text="Left" value="flex-start"/>
                                 <center jcr:primaryType="nt:unstructured" text="Center" value="center"/>
                                 <flex-end jcr:primaryType="nt:unstructured" text="Right" value="flex-end"/>
                              </items>
                           </textAlignHeader>
                        </items>
                     </column>
                  </items>
               </header-properties>
               <hmenu-nav-properties jcr:primaryType="nt:unstructured"
                                     jcr:title="Menu Nav"
                                     sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <fontFamilyNav cq-msm-lockable="./fontFamilyNav"
                                          fieldLabel="Font Family:"
                                          jcr:primaryType="nt:unstructured"
                                          name="./fontFamilyNav"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                          vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyNav"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyNav"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyNav"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyNav>
                           <bgColorSpanNav cq-msm-lockable="./bgColorSpanNav"
                                           fieldLabel="Select border color"
                                           jcr:primaryType="nt:unstructured"
                                           name="./bgColorSpanNav"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <textNavColor cq-msm-lockable="./textNavColor"
                                         fieldLabel="Select text color"
                                         jcr:primaryType="nt:unstructured"
                                         name="./textNavColor"
                                         sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </hmenu-nav-properties>
               <basic-properties jcr:primaryType="nt:unstructured"
                                 jcr:title="Basic"
                                 sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <titleTextBasic cq-msm-lockable="./titleTextBasic"
                                           fieldLabel="Title Basic Tab"
                                           jcr:primaryType="nt:unstructured"
                                           name="./titleTextBasic"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <fontSizeTitleBasic cq-msm-lockable="./fontSizeTitleBasic"
                                               fieldLabel="Title Font-Size Basic Tab (px)"
                                               jcr:primaryType="nt:unstructured"
                                               name="./fontSizeTitleBasic"
                                               sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                           <fontFamilyTitleBasic cq-msm-lockable="./fontFamilyTitleBasic"
                                                 fieldLabel="Title Font Family Basic Tab:"
                                                 jcr:primaryType="nt:unstructured"
                                                 name="./fontFamilyTitleBasic"
                                                 sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                 vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleBasic"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleBasic"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyTitleBasic"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyTitleBasic>
                           <titleColorBasic cq-msm-lockable="./titleColorBasic"
                                            fieldLabel="Color Basic Tab Text"
                                            jcr:primaryType="nt:unstructured"
                                            name="./titleColorBasic"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelFullname cq-msm-lockable="./labelFullname"
                                          fieldLabel="Label first input"
                                          jcr:primaryType="nt:unstructured"
                                          name="./labelFullname"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <placeholderFullname cq-msm-lockable="./placeholderFullname"
                                                fieldLabel="Placeholder first input"
                                                jcr:primaryType="nt:unstructured"
                                                name="./placeholderFullname"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <spanFullname cq-msm-lockable="./spanFullname"
                                         fieldLabel="Span first input"
                                         jcr:primaryType="nt:unstructured"
                                         name="./spanFullname"
                                         sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelNickname cq-msm-lockable="./labelNickname"
                                          fieldLabel="Label second input"
                                          jcr:primaryType="nt:unstructured"
                                          name="./labelNickname"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <placeholderNickname cq-msm-lockable="./placeholderNickname"
                                                fieldLabel="Label second input"
                                                jcr:primaryType="nt:unstructured"
                                                name="./placeholderNickname"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <LabelEmail cq-msm-lockable="./LabelEmail"
                                       fieldLabel="Email Label"
                                       jcr:primaryType="nt:unstructured"
                                       name="./LabelEmail"
                                       sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <PlaceholderEmail cq-msm-lockable="./PlaceholderEmail"
                                             fieldLabel="Placeholder Email"
                                             jcr:primaryType="nt:unstructured"
                                             name="./PlaceholderEmail"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <InvalidTextEmail cq-msm-lockable="./InvalidTextEmail"
                                             fieldLabel="Span Email"
                                             jcr:primaryType="nt:unstructured"
                                             name="./InvalidTextEmail"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <LabelPhone cq-msm-lockable="./LabelPhone"
                                       fieldLabel="Phone Label"
                                       jcr:primaryType="nt:unstructured"
                                       name="./LabelPhone"
                                       sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelBirthday cq-msm-lockable="./labelBirthday"
                                          fieldLabel="Birthday Label"
                                          jcr:primaryType="nt:unstructured"
                                          name="./labelBirthday"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <spanBirthday cq-msm-lockable="./spanBirthday"
                                         fieldLabel="Birthday Span"
                                         jcr:primaryType="nt:unstructured"
                                         name="./spanBirthday"
                                         sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelCheckbox cq-msm-lockable="./labelCheckbox"
                                          fieldLabel="Checkbox Label"
                                          jcr:primaryType="nt:unstructured"
                                          name="./labelCheckbox"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <msgZero cq-msm-lockable="./msgZero"
                                    fieldLabel="Button text"
                                    jcr:primaryType="nt:unstructured"
                                    name="./msgZero"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <backgroundButtonZero cq-msm-lockable="./backgroundButtonZero"
                                                 fieldLabel="Button background color"
                                                 jcr:primaryType="nt:unstructured"
                                                 name="./backgroundButtonZero"
                                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </basic-properties>
               <social-properties jcr:primaryType="nt:unstructured"
                                  jcr:title="Social"
                                  sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <titleTextSocial cq-msm-lockable="./titleTextSocial"
                                            fieldLabel="Title Social Tab"
                                            jcr:primaryType="nt:unstructured"
                                            name="./titleTextSocial"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <fontSizeTitleSocial cq-msm-lockable="./fontSizeTitleSocial"
                                                fieldLabel="Title Font-Size Social Tab (px)"
                                                jcr:primaryType="nt:unstructured"
                                                name="./fontSizeTitleSocial"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                           <fontFamilyTitleSocial cq-msm-lockable="./fontFamilyTitleSocial"
                                                  fieldLabel="Title Font Family Social Tab:"
                                                  jcr:primaryType="nt:unstructured"
                                                  name="./fontFamilyTitleSocial"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                  vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleSocial"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleSocial"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyTitleSocial"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyTitleSocial>
                           <titleColorSocial cq-msm-lockable="./titleColorSocial"
                                             fieldLabel="Color Social Tab Text"
                                             jcr:primaryType="nt:unstructured"
                                             name="./titleColorSocial"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelLinkedin cq-msm-lockable="./labelLinkedin"
                                          fieldLabel="Linkedin Label"
                                          jcr:primaryType="nt:unstructured"
                                          name="./labelLinkedin"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <placeholderLinkedin cq-msm-lockable="./placeholderLinkedin"
                                                fieldLabel="Linkedin Placeholder"
                                                jcr:primaryType="nt:unstructured"
                                                name="./placeholderLinkedin"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </social-properties>
               <certificates-properties jcr:primaryType="nt:unstructured"
                                        jcr:title="Certificates"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <titleTextCertificates cq-msm-lockable="./titleTextCertificates"
                                                  fieldLabel="Title Certificates Tab"
                                                  jcr:primaryType="nt:unstructured"
                                                  name="./titleTextCertificates"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <fontSizeTitleCertificates cq-msm-lockable="./fontSizeTitleCertificates"
                                                      fieldLabel="Title Font-Size Certificates Tab (px)"
                                                      jcr:primaryType="nt:unstructured"
                                                      name="./fontSizeTitleCertificates"
                                                      sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                           <fontFamilyTitleCertificates cq-msm-lockable="./fontFamilyTitleCertificates"
                                                        fieldLabel="Title Font Family Certificates Tab:"
                                                        jcr:primaryType="nt:unstructured"
                                                        name="./fontFamilyTitleCertificates"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                        vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleCertificates"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleCertificates"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyTitleCertificates"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyTitleCertificates>
                           <titleColorCertificates cq-msm-lockable="./titleColorCertificates"
                                                   fieldLabel="Color Certificates Tab Text"
                                                   jcr:primaryType="nt:unstructured"
                                                   name="./titleColorCertificates"
                                                   sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <labelCertificates cq-msm-lockable="./labelCertificates"
                                              fieldLabel="Label Input"
                                              jcr:primaryType="nt:unstructured"
                                              name="./labelCertificates"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <placeholderCertificates cq-msm-lockable="./placeholderCertificates"
                                                    fieldLabel="Placeholder First Input"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./placeholderCertificates"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </certificates-properties>
               <success-properties jcr:primaryType="nt:unstructured"
                                   jcr:title="Success"
                                   sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <titleTextSuccess cq-msm-lockable="./titleTextSuccess"
                                             fieldLabel="Title Succes Tab"
                                             jcr:primaryType="nt:unstructured"
                                             name="./titleTextSuccess"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <fontSizeTitleSuccess cq-msm-lockable="./fontSizeTitleSuccess"
                                                 fieldLabel="Title Font-Size Succes Tab (px)"
                                                 jcr:primaryType="nt:unstructured"
                                                 name="./fontSizeTitleSuccess"
                                                 sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                           <fontFamilyTitleSuccess cq-msm-lockable="./fontFamilyTitleSuccess"
                                                   fieldLabel="Title Font Family Succes Tab:"
                                                   jcr:primaryType="nt:unstructured"
                                                   name="./fontFamilyTitleSuccess"
                                                   sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                   vertical="{Boolean}false">
                              <items jcr:primaryType="nt:unstructured">
                                 <Inter checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleSuccess"
                                        text="Inter"
                                        value="Inter"/>
                                 <Arial checked="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        name="fontFamilyTitleSuccess"
                                        text="Arial"
                                        value="Arial"/>
                                 <Nunito checked="{Boolean}true"
                                         jcr:primaryType="nt:unstructured"
                                         name="fontFamilyTitleSuccess"
                                         text="Nunito"
                                         value="Nunito"/>
                              </items>
                           </fontFamilyTitleSuccess>
                           <titleColorSuccess cq-msm-lockable="./titleColorSuccess"
                                              fieldLabel="Color Succes Tab Title"
                                              jcr:primaryType="nt:unstructured"
                                              name="./titleColorSuccess"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </success-properties>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>
